let mapleader = " "
set ignorecase
set clipboard=unnamed
set hlsearch
set incsearch

map <C-p> :vsc ReSharper.ReSharper_GotoType<CR>

map <Leader><Leader> :vsc Tools.InvokeAceJumpCommand<CR>
map <Leader>b :vsc Build.BuildSolution<CR>
map <Leader>c :vsc ReSharper.ReSharper_DuplicateText<CR>
map <Leader>cf :vsc Edit.FormatDocument<CR>
map <Leader>ck :vsc Tools.CustomizeKeyboard<CR>
map <Leader>cs :vsc Edit.CommentSelection<CR>
map <Leader>d :vsc Debug.AttachtoProcess<CR>
map <Leader>D :vsc Debug.ReattachtoProcess<CR>
map <Leader>E :vsc ReSharper.ReSharper_GotoPrevErrorInSolution<CR>
map <Leader>e :vsc ReSharper.ReSharper_GotoNextErrorInSolution<CR>
map <Leader>em :vsc ReSharper.ReSharper_ExtractMethod<CR>
map <Leader>f :vsc ReSharper.ReSharper_GotoFileMember<CR>
map <Leader>g :vsc ReSharper.ReSharper_GotoText<CR>
map <Leader>h :vsc Window.PreviousTab<CR>
map <Leader>k :vsc ReSharper.ReSharper_GotoPrevMethod<CR>
map <Leader>j :vsc ReSharper.ReSharper_GotoNextMethod<CR>
map <Leader>l :vsc Window.NextTab<CR>
map <Leader>mo :vsc ReSharper.ReSharper_Move<CR>
map <Leader>n :vsc Window.SolutionExplorerSearch<CR>
map <Leader>pb :vsc Edit.PeekBackward<CR>
map <Leader>pd :vsc Edit.PeekDefinition<CR>
map <Leader>pf :vsc Edit.PeekForward<CR>
map <Leader>q :vsc File.Close<CR>
map <Leader>qa :vsc ReSharper_CloseAllCloseAll<CR>
map <Leader>qab :vsc File.CloseAllButThis<CR>
map <Leader>qabp :vsc ReSharper_CloseAllCloseAllButPinned<CR>
map <Leader>r :vsc ReSharper.ReSharper_Rename<CR>
map <Leader>s :vsc ReSharper.ReSharper_SurroundWith<CR>
map <Leader>sd :vsc Debug.StopDebugging<CR>
map <Leader>t :vsc ReSharper.ReSharper_GotoType<CR>
map <Leader>ta :vsc ReSharper.ReSharper_UnitTestRunSolution<CR>
map <Leader>td :vsc ReSharper.ReSharper_UnitTestDebugContext<CR>
map <Leader>tl :vsc ReSharper.ReSharper_UnitTestSessionRepeatPreviousRun<CR>
map <Leader>tr :vsc ReSharper.ReSharper_UnitTestRunFromContext<CR>
map <Leader>tt :vsc ReSharper.ReSharper_ShowUnitTestSessions<CR>
map <Leader>ut :vsc Edit.UntabifySelectedLines<CR>
map <Leader>, :vsc ReSharper.ReSharper_GotoText<CR>
map <Leader>/ :vsc ReSharper.ReSharper_LineComment<CR>
map <Leader>; A;<Esc>
map gd :vsc ReSharper.ReSharper_GotoDeclaration<CR>
map gu :vsc ReSharper.ReSharper_GotoUsage<CR>
map zl :so ~/.vsvimrc<CR>

nmap <Leader>q :q<CR>
nmap <Leader>w :w<CR>
