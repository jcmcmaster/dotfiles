[difftool "meld"]
        # cmd = meld.exe "$(wslpath -aw $LOCAL)" "$(wslpath -aw $REMOTE)"
        # cmd = meld.exe $LOCAL $REMOTE
        cmd = meld.exe \"$(wslpath -aw $LOCAL)\" \"$(wslpath -aw $REMOTE)\"
[difftool "bc"]
        cmd = '/mnt/c/Program Files/Beyond Compare 5/BComp.exe' $(wslpath -aw $LOCAL) $(wslpath -aw $REMOTE)
[difftool "vimdirdiff"]
        cmd = nvim -f '+next' '+execute \"DirDiff\" argv(0) argv(1)' $LOCAL $REMOTE
[difftool "nvim"]
        cmd = "nvim -d \"$LOCAL\" \"$REMOTE\""
