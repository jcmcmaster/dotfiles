        cmd = '/mnt/c/Program Files/Meld/Meld.exe' \"$(wslpath -aw $LOCAL)\" \"$(wslpath -aw $MERGED)\" \"$(wslpath -aw $REMOTE)\" --output \"$(wslpath -aw $MERGED)\"
